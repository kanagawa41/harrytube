<h1>User Infos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Nickname</th>
      <th>Sex</th>
      <th>Icon</th>
      <th>Breed history</th>
      <th>Pet name</th>
      <th>Pet type</th>
      <th>Pet sex</th>
      <th>Youtube channel</th>
      <th>Twitter</th>
      <th>Blog url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_infos.each do |user_info| %>
      <tr>
        <td><%= user_info.user %></td>
        <td><%= user_info.nickname %></td>
        <td><%= user_info.sex %></td>
        <td><%= user_info.icon %></td>
        <td><%= user_info.breed_history %></td>
        <td><%= user_info.pet_name %></td>
        <td><%= user_info.pet_type %></td>
        <td><%= user_info.pet_sex %></td>
        <td><%= user_info.youtube_channel_id %></td>
        <td><%= user_info.twitter_id %></td>
        <td><%= user_info.blog_url %></td>
        <td><%= link_to 'Show', user_info %></td>
        <td><%= link_to 'Edit', edit_user_info_path(user_info) %></td>
        <td><%= link_to 'Destroy', user_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Info', new_user_info_path %>
