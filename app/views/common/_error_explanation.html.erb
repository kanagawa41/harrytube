<div id="error_explanation">
  <div class="card">
    <h5 class="card-header"><%= t('views.common.error_field.error_title') %></h5>
    <div class="card-body">
      <div class="input-group">
        <ul>
        <% form.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
